{
  "dashboard": {
    "id": null,
    "title": "Expenses Bot Logs",
    "tags": ["expenses-bot", "logs", "telegram"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Telegram Commands & Callbacks",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"expenses-bot\"} |= \"Telegram request received\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending",
          "dedupStrategy": "none"
        }
      },
      {
        "id": 2,
        "title": "User Commands (/start, /menu, etc.)",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"expenses-bot\"} |= \"updateType\":\"message\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending",
          "dedupStrategy": "none"
        }
      },
      {
        "id": 3,
        "title": "Bot Responses & Actions",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"expenses-bot\"} |= \"Telegram request completed\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending",
          "dedupStrategy": "none"
        }
      },
      {
        "id": 4,
        "title": "Database Operations",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"expenses-bot\"} |= \"Database query\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending",
          "dedupStrategy": "none"
        }
      },
      {
        "id": 5,
        "title": "Errors & Warnings",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"expenses-bot\"} |= \"ERROR\" or |= \"WARN\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending",
          "dedupStrategy": "none"
        }
      },
      {
        "id": 6,
        "title": "User Activity by Username",
        "type": "stat",
        "targets": [
          {
            "expr": "count_over_time({job=\"expenses-bot\"} |= \"Telegram request received\" [1h]) by (username)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Commands Frequency (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "count_over_time({job=\"expenses-bot\"} |= \"updateType\":\"message\" [1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "stat"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Callbacks Frequency (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "count_over_time({job=\"expenses-bot\"} |= \"updateType\":\"callback_query\" [1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "stat"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
} 